{
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "friendsofsymfony/comment-bundle": "3.0.x-dev",
        "friendsofsymfony/jsrouting-bundle": "^2.0",
        "harmony-project/ui-bundle": "^1.1",
        "irazasyed/telegram-bot-sdk": "^2.2",
        "league/tactician-bundle": "^1.1",
        "league/tactician-doctrine": "^1.1",
        "liip/imagine-bundle": "^1.9",
        "ornicar/gravatar-bundle": "^1.1",
        "sensio/framework-extra-bundle": "^5.0",
        "sensiolabs/security-checker": "^4.1",
        "sgomez/ssp-guard-bundle": "^0.9",
        "sonata-project/admin-bundle": "^3.0",
        "sonata-project/doctrine-orm-admin-bundle": "^3.0",
        "sonata-project/formatter-bundle": "^3.0",
        "sonata-project/user-bundle": "^4.0",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/asset": "^3.4",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^3.4",
        "symfony/lts": "^3",
        "symfony/maker-bundle": "^1.0",
        "symfony/orm-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.0",
        "symfony/twig-bundle": "^3.4",
        "symfony/validator": "^3.4",
        "symfony/webpack-encore-pack": "^1.0",
        "symfony/yaml": "^3.4",
        "vich/uploader-bundle": "^1.6",
        "white-october/pagerfanta-bundle": "^1.0"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.2",
        "friendsofphp/php-cs-fixer": "^2.4",
        "hautelook/alice-bundle": "^2.0",
        "nelmio/alice": "^3.0",
        "symfony/debug-bundle": "^3.3",
        "symfony/debug-pack": "^1.0",
        "symfony/dotenv": "^3.3",
        "symfony/web-server-bundle": "^3.3",
        "theofidry/alice-data-fixtures": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BTZNBWHBW8B65JX999E0TP91",
            "allow-contrib": true
        }
    }
}
