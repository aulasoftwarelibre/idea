{
    "name": "aulasoftwarelibre/idea",
    "description": "Gestor de actividades",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "botman/driver-telegram": "^1.5",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "exercise/htmlpurifier-bundle": "2.0.x-dev",
        "friendsofsymfony/ckeditor-bundle": "^1.1",
        "friendsofsymfony/comment-bundle": "^2.2",
        "friendsofsymfony/jsrouting-bundle": "^2.2",
        "harmony-project/ui-bundle": "^1.1",
        "hwi/oauth-bundle": "0.6.*",
        "leogout/seo-bundle": "^1.2",
        "liip/imagine-bundle": "^2.1",
        "ornicar/gravatar-bundle": "^1.3",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/httplug-bundle": "^1.9",
        "phpoffice/phpspreadsheet": "^1.4",
        "ramsey/uuid": "^3.8",
        "sensio/framework-extra-bundle": "^5.1",
        "sensiolabs/security-checker": "^4.1",
        "sgomez/botman-bundle": "^0.1.0",
        "sonata-project/admin-bundle": "^3.38",
        "sonata-project/doctrine-orm-admin-bundle": "^3.6",
        "sonata-project/formatter-bundle": "4.x-dev",
        "sonata-project/intl-bundle": "^2.5",
        "sonata-project/user-bundle": "^4.2",
        "spomky-labs/jose": "^7.1",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/asset": "^4.1",
        "symfony/console": "^4.1",
        "symfony/expression-language": "^4.1",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/lts": "^4@dev",
        "symfony/messenger": "^4.1",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "^4.1",
        "symfony/psr-http-message-bridge": "^1.0",
        "symfony/security-bundle": "^4.1",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/twig-bundle": "^4.1",
        "symfony/validator": "^4.1",
        "symfony/web-link": "^4.1",
        "symfony/webpack-encore-pack": "^1.0",
        "symfony/yaml": "^4.1",
        "vich/uploader-bundle": "^1.8",
        "white-october/pagerfanta-bundle": "^1.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10.3",
        "phpstan/phpstan-doctrine": "^0.10.0",
        "phpstan/phpstan-symfony": "^0.10.1",
        "symfony/debug-pack": "*",
        "symfony/dotenv": "^4.1",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "^1.0",
        "symfony/web-server-bundle": "^4.1",
        "symplify/easy-coding-standard": "^4.6"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "check-cs": "vendor/bin/ecs check src",
        "fix-cs": "vendor/bin/ecs check --fix src",
        "phpstan": "vendor/bin/phpstan analyse src --level max"
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BTZNBWHBW8B65JX999E0TP91",
            "allow-contrib": true
        }
    }
}
