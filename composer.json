{
    "name": "aulasoftwarelibre/idea",
    "description": "Gestor de actividades",
    "type": "project",
    "license": "AGPL-3.0-or-later",
    "require": {
        "php": "^7.4",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "antishov/doctrine-extensions-bundle": "^1.3",
        "cocur/slugify": "^4.0",
        "codedmonkey/semantic-ui-bundle": "^2.0",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": "^1.0",
        "doctrine/common": "^2",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "doctrine/orm": "^2.7",
        "easycorp/easyadmin-bundle": "^3.2",
        "exercise/htmlpurifier-bundle": "^2.0",
        "friendsofsymfony/ckeditor-bundle": "^1.0",
        "friendsofsymfony/comment-bundle": "^2.2",
        "friendsofsymfony/jsrouting-bundle": "^2.2",
        "informaticauco/oauth2-uco": "^0.5.0",
        "knpuniversity/oauth2-client-bundle": "^1.28",
        "league/oauth2-github": "^2.0",
        "league/oauth2-google": "^3.0",
        "leogout/seo-bundle": "^1.2",
        "liip/imagine-bundle": "^2.1",
        "phpdocumentor/reflection-docblock": "^5.1",
        "phpoffice/phpspreadsheet": "^1.4",
        "sensio/framework-extra-bundle": "^5.1",
        "spatie/calendar-links": "^1.2",
        "symfony/asset": "*",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/expression-language": "*",
        "symfony/flex": "^1.1",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/mailer": "*",
        "symfony/messenger": "*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "*",
        "symfony/property-access": "4.4.*",
        "symfony/property-info": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer": "4.4.*",
        "symfony/templating": "4.4.*",
        "symfony/translation": "*",
        "symfony/twig-bundle": "*",
        "symfony/validator": "*",
        "symfony/web-link": "*",
        "symfony/webpack-encore-bundle": "^1.5",
        "symfony/yaml": "*",
        "twig/cssinliner-extra": "^2.12",
        "twig/extensions": "^1.5",
        "twig/extra-bundle": "^2.12",
        "twig/inky-extra": "^2.12",
        "vich/uploader-bundle": "^1.8",
        "wohali/oauth2-discord-new": "^1.1"
    },
    "require-dev": {
        "doctrine/coding-standard": "^8.1",
        "overtrue/phplint": "^2.1",
        "phpmd/phpmd": "^2.9",
        "psalm/plugin-symfony": "^1.5",
        "symfony/browser-kit": "^4.4",
        "symfony/css-selector": "^4.4",
        "symfony/debug-bundle": "^4.4",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/stopwatch": "^4.4",
        "symfony/var-dumper": "^4.4",
        "symfony/web-profiler-bundle": "^4.4",
        "symfony/web-server-bundle": "*",
        "vimeo/psalm": "^3.17"
    },
    "config": {
        "platform": {
            "php": "7.4.10"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "ckeditor:install --clear=drop": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test": [
            "@phplint",
            "@phpcs",
            "@psalm",
            "@phpunit"
        ],
        "phplint": "phplint",
        "phpcs": "phpcs",
        "phpcbf": "phpcbf",
        "phpmd": "phpmd src,tests text phpmd.xml --exclude 'src/Kernel.php'",
        "psalm": "psalm --show-info=false",
        "phpunit": "simple-phpunit"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "require": "4.4.*",
            "allow-contrib": false
        }
    }
}
